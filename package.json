{
  "name": "tiny-url",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prisma:migrate": "prisma migrate dev --schema=src/database/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/database/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=src/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/database/prisma/schema.prisma"
  },
  "keywords": [
    "typescript",
    "express",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.9",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.1",
    "prisma": "^6.1.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2"
  }
}
